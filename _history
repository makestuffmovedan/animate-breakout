2)\n            frame += 1\n        else:\n            frame = 0\n            serial.write_line(\"LOOP\")\ndef record():\n    global anim_frames, knob_p0, knob_p1, knob_p2, servo_13, servo_14, servo_15, anim_frame\n    anim_frames = []\n    while recording == 1:\n        strip.show_color(neopixel.colors(NeoPixelColors.RED))\n        knob_p0 = pins.analog_read_pin(AnalogReadWritePin.P0)\n        knob_p1 = pins.analog_read_pin(AnalogReadWritePin.P1)\n        knob_p2 = pins.analog_read_pin(AnalogReadWritePin.P2)\n        servo_13 = Math.map(knob_p0, 0, 1023, 0, 180)\n        servo_14 = Math.map(knob_p1, 0, 1023, 0, 180)\n        servo_15 = Math.map(knob_p2, 0, 1023, 0, 180)\n        pins.servo_write_pin(AnalogPin.P13, servo_13)\n        pins.servo_write_pin(AnalogPin.P14, servo_14)\n        pins.servo_write_pin(AnalogPin.P15, servo_15)\n        anim_frame = [knob_p0, knob_p1, knob_p2]\n        anim_frames.append(anim_frame)\n        serial.write_line(\"\" + str((anim_frame[0])))\n        basic.pause(42)\n\ndef on_button_pressed_b():\n    global playing, recording, frame\n    if playing:\n        playing = 0\n        serial.write_line(\"STOP - PLAY\")\n        strip.show_color(neopixel.colors(NeoPixelColors.WHITE))\n    else:\n        playing = 1\n        recording = 0\n        frame = 0\n        strip.show_color(neopixel.colors(NeoPixelColors.GREEN))\ninput.on_button_pressed(Button.B, on_button_pressed_b)\n\ndef sensorLevel():\n    global sensor_level, sensor_previous\n    if sensor_current < sensor_max and sensor_current > sensor_min:\n        if sensor_current > sensor_previous:\n            sensor_level = sensor_current\n    sensor_level += -40\n    sensor_previous = sensor_current\n    serial.write_string(\"\" + str((sensor_level)))\ndef move():\n    global knob_p0, knob_p1, knob_p2, sensor_current, servo_13, servo_14, servo_15, colour_r, colour_g, colour_b\n    basic.pause(10)\n    knob_p0 = pins.analog_read_pin(AnalogReadWritePin.P0)\n    knob_p1 = pins.analog_re